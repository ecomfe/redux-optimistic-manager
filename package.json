{
  "name": "redux-optimistic-manager",
  "version": "1.0.0",
  "description": "A transaction based optimistic base manager for redux Edit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha",
    "test-cov": "npm run lint && cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov --reporter=html mocha",
    "report-cov": "cat coverage/lcov.info | coveralls",
    "ci": "npm run test-cov && npm run compile",
    "lint": "fecs",
    "doc": "esdoc -c esdoc.json",
    "compile": "gulp",
    "clean": "gulp clean",
    "prepublish": "npm run compile",
    "start": "cross-env NODE_ENV=example babel-node example/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomfe/redux-optimistic-manager.git"
  },
  "keywords": [
    "redux",
    "optimistic",
    "infrastructure",
    "redux-middleware"
  ],
  "author": "otakustay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomfe/redux-optimistic-manager/issues"
  },
  "homepage": "https://github.com/ecomfe/redux-optimistic-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^4.0.0",
    "fecs": "^1.2.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "mocha": "^3.3.0",
    "nyc": "^10.3.0",
    "redux": "^3.6.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-stream": "^1.19.0",
    "sinon": "^2.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
